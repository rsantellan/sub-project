{{ form_start(form, {'attr' : { 'role' : 'form'} }) }}
<div class="form-group">
    {{ form_errors(form.email) }}
    <label for="{{ form.email.vars.id }}">Email</label>
    {{ form_widget(form.email, {'attr' : {'class' : 'form-control'}}) }}
</div>
<div class="form-group">
    {{ form_errors(form.userRoles) }}
    <label for="{{ form.userRoles.vars.id }}">Permisos</label>
    {{ form_widget(form.userRoles, {'attr' : {'class' : 'form-control'}}) }}
</div>
{{ form_rest(form) }}
<div class="form-group">
    <button type="submit" class="btn btn-default"><i class="fa fa-save"></i>Guardar</button>
</div>
{{ form_end(form) }}
